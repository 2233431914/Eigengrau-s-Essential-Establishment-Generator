{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": 0,
    "indent": 1,
    "linebreak-style": 0,
    "object-curly-spacing": 0,
    "arrow-body-style": 0,
    "no-console": 0,
    "max-len": 0,
    "prefer-template": 0,
    "import/no-unresolved": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "semi": 0,
    "space-before-blocks": 0
  }
}
